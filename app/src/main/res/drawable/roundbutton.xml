<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#f5a82c" />
            <gradient android:angle="-90" android:startColor="#e1b02d" android:endColor="#eaa720"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#d9ab2a" />
            <solid android:color="#f1b706"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#e7ae3b" />
            <gradient android:angle="-90" android:startColor="#f7c514" android:endColor="#dcae07" />
        </shape>
    </item>
</selector>